<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ra.boomblog.picture.mapper.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ra.boomblog.commons.entity.File">
        <result column="uid" property="uid"/>
        <result column="file_old_name" property="fileOldName"/>
        <result column="file_size" property="fileSize"/>
        <result column="file_sort_uid" property="fileSortUid"/>
        <result column="pic_expanded_name" property="picExpandedName"/>
        <result column="pic_name" property="picName"/>
        <result column="pic_url" property="picUrl"/>
        <result column="admin_uid" property="adminUid"/>
        <result column="user_uid" property="userUid"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uid
        , file_old_name, file_size, file_sort_uid, pic_expanded_name, pic_name, pic_url, admin_uid, user_id,
        create_time, update_time, status
    </sql>

</mapper>
